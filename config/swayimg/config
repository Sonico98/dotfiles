[general]
mode = viewer
size = fullscreen

[viewer]
scale = fit
antialiasing = none

[list]
all = yes
order = alpha

[info]
show = no

[font]
name = GoogleSans Nerd Font
size = 14
color = #FFFFFF
shadow = #000000

[keys.viewer]
j = zoom -10
k = zoom +10
h = prev_file
l = next_file
0 = first_file
9 = last_file
Shift+j = step_down 10
Shift+k = step_up 10
Shift+h = step_left 10
Shift+l = step_right 10
a = antialiasing
i = info full
Shift+i = info none
R = reload
q = exit
Right = rotate_right
Left = rotate_left
f = fullscreen
v = flip_vertical
V = flip_horizontal
s = animation
BackSpace = zoom optimal
